h) {\n    @content;\n  }\n}\n\n@mixin column($screen-width, $count) {\n  flex-shrink: 0;\n  padding-right: 1rem;\n  padding-bottom: 1rem;\n  padding-left: 1rem;\n\n  @include media-breakpoint-up($screen-width) {\n    flex-basis: (100% / 12 * $count);\n  }\n}\n\n@mixin firefox-only {\n  @at-root {\n    @-moz-document url-prefix() {\n      & {\n        @content;\n      }\n    }\n  }\n}*/","../../components/text-field/text-field.scss":"@import '../../theme/variables.scss';\n\n.text-field {\n  width: fit-content;\n  position: relative;\n\n  .title_and_status {\n    \n    display: flex;\n    justify-content: space-between;\n\n    h3.title {\n      float: left;\n    }\n\n    h3.status {\n      color: $color-dark-50;\n    }\n  }\n\n  input.field {\n    width: 320px;\n    height: 44px;\n    background-color: #FFFFFF;\n    border: 1px solid $color-dark-25;\n    border-radius: 4px;\n    padding-left: 15px;\n    transition: all 0.2s ease-in;\n\n    &::placeholder {\n      color: $color-dark-25;\n      transition: all 0.2s ease-in;\n    }\n\n    &:hover {\n      transition: all 0.5s ease;\n      border: 1px solid $color-dark-50;\n\n      &::placeholder {\n        color: $color-dark-75;\n      }\n    }\n  }\n  input.subscription {\n    width: 266px;\n  }\n  button[type=\"submit\"]{\n    height: 18px;\n    position: absolute;\n    bottom: 12px;\n    right: 15px;\n    content: '';\n    outline: none;\n\n    cursor: pointer;\n    border: none;\n  }\n  \n}\n\n.text-field {\n  input.field.hover {\n    border: 1px solid $color-dark-50;\n    &::placeholder {\n      color: $color-dark-75;\n\n    }\n  }\n}\n\n"},"lineCount":null}},"error":null,"hash":"4e9c9205bb5e1887fc814d129c1ed5b0","cacheData":{"env":{}}}